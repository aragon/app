# This file is for unifying the coding style for different editors and IDEs
# editorconfig.org

root = true

[*]
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true
indent_style = space
indent_size = 4
max_line_length = 120
quote_type = single

[*.{css,scss,json,json5,yml}]
indent_size = 2

[{FastFile,Appfile}]
indent_size = 2

[*.{html,json}]
max_line_length = off

[*.{ts,tsx}]
max_line_length = 120
